{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "apiServer": {
            "type": "object",
            "properties": {
                "enableAdmissionPlugins": {
                    "type": "string"
                },
                "featureGates": {
                    "type": "string"
                }
            }
        },
        "baseDomain": {
            "type": "string"
        },
        "bastion": {
            "type": "object",
            "properties": {
                "bootFromVolume": {
                    "type": "boolean"
                },
                "diskSize": {
                    "type": "integer"
                },
                "flavor": {
                    "type": "string"
                },
                "image": {
                    "type": "string"
                }
            }
        },
        "cloudConfig": {
            "type": "string"
        },
        "cloudName": {
            "type": "string"
        },
        "clusterDescription": {
            "type": "string"
        },
        "clusterName": {
            "type": "string"
        },
        "controlPlane": {
            "type": "object",
            "properties": {
                "availabilityZones": {
                    "type": "array"
                },
                "bootFromVolume": {
                    "type": "boolean"
                },
                "diskSize": {
                    "type": "integer"
                },
                "etcd": {
                    "type": "object",
                    "properties": {
                        "imageRepository": {
                            "type": "string"
                        },
                        "imageTag": {
                            "type": "string"
                        }
                    }
                },
                "flavor": {
                    "type": "string"
                },
                "image": {
                    "type": "string"
                },
                "replicas": {
                    "type": "integer"
                }
            }
        },
        "controllerManager": {
            "type": "object",
            "properties": {
                "featureGates": {
                    "type": "string"
                }
            }
        },
        "dnsNameservers": {
            "type": "array"
        },
        "externalNetworkID": {
            "type": "string"
        },
        "ignition": {
            "type": "object",
            "properties": {
                "enable": {
                    "type": "boolean"
                }
            }
        },
        "includeClusterResourceSet": {
            "type": "string"
        },
        "kubectlImage": {
            "type": "object"
        },
        "kubernetesVersion": {
            "type": "string"
        },
        "managementCluster": {
            "type": "string"
        },
        "networkName": {
            "type": "string"
        },
        "nodeCIDR": {
            "type": "string"
        },
        "nodeClasses": {
            "type": "object"
        },
        "nodePools": {
            "type": "object"
        },
        "oidc": {
            "type": "object",
            "properties": {
                "caFile": {
                    "type": "string"
                },
                "clientId": {
                    "type": "string"
                },
                "groupsClaim": {
                    "type": "string"
                },
                "issuerUrl": {
                    "type": "string"
                },
                "usernameClaim": {
                    "type": "string"
                },
                "usernamePrefix": {
                    "type": "string"
                }
            }
        },
        "organization": {
            "type": "string"
        },
        "subnetName": {
            "type": "string"
        }
    },
    "required": [
              "apiServer",
              "baseDomain",
              "bastion",
              "cloudConfig",
              "cloudName",
              "clusterDescription",
              "clusterName",
              "controllerManager",
              "controlPlane",
              "kubernetesVersion",
              "managementCluster",
              "nodeCIDR",
              "nodeClasses",
              "nodePools",
              "organization"
          ]
}
